{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "tako",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "shebang"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_statement"
          }
        }
      ]
    },
    "_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "SYMBOL",
          "name": "_block"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "_sequence": {
      "type": "REPEAT",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_statement"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "STRING",
            "value": ";"
          }
        ]
      }
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "definition"
        },
        {
          "type": "SYMBOL",
          "name": "_block"
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        }
      ]
    },
    "definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "ident"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        }
      ]
    },
    "expression": {
      "type": "SYMBOL",
      "name": "_inner_expression"
    },
    "_inner_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "("
            },
            {
              "type": "SYMBOL",
              "name": "expression"
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "color"
        },
        {
          "type": "SYMBOL",
          "name": "ident"
        }
      ]
    },
    "number": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "int_literal"
        },
        {
          "type": "SYMBOL",
          "name": "float_literal"
        }
      ]
    },
    "comment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "single_line_comment"
        },
        {
          "type": "SYMBOL",
          "name": "nesting_comment"
        }
      ]
    },
    "_nesting_comment_contents": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "nesting_comment"
        },
        {
          "type": "SYMBOL",
          "name": "_anything"
        }
      ]
    },
    "nesting_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "/*"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_nesting_comment_contents"
          }
        },
        {
          "type": "STRING",
          "value": "*/"
        }
      ]
    },
    "_anything": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "STRING",
          "value": "/"
        },
        {
          "type": "PATTERN",
          "value": "[^*/]"
        }
      ]
    },
    "break": {
      "type": "STRING",
      "value": "break"
    },
    "continue": {
      "type": "STRING",
      "value": "continue"
    },
    "return": {
      "type": "STRING",
      "value": "return"
    },
    "forall": {
      "type": "STRING",
      "value": "forall"
    },
    "exists": {
      "type": "STRING",
      "value": "exists"
    },
    "given": {
      "type": "STRING",
      "value": "given"
    },
    "color": {
      "type": "PATTERN",
      "value": "#[a-fA-F0-9_]+"
    },
    "ident": {
      "type": "PATTERN",
      "value": "[a-zA-Z][a-zA-Z0-9_]*"
    },
    "string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "escape_sequence"
              },
              {
                "type": "PATTERN",
                "value": "[^\\\"]"
              }
            ]
          }
        },
        {
          "type": "IMMEDIATE_TOKEN",
          "content": {
            "type": "STRING",
            "value": "\""
          }
        }
      ]
    },
    "escape_sequence": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\\"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^xu]"
              },
              {
                "type": "PATTERN",
                "value": "u[0-9a-fA-F]{4}"
              },
              {
                "type": "PATTERN",
                "value": "u\\{[0-9a-fA-F]+\\}"
              },
              {
                "type": "PATTERN",
                "value": "x[0-9a-fA-F]{2}"
              }
            ]
          }
        ]
      }
    },
    "int_literal": {
      "type": "PATTERN",
      "value": "[0-9][0-9_]*"
    },
    "float_literal": {
      "type": "PATTERN",
      "value": "[0-9][0-9_]*\\.[0-9_]*"
    },
    "shebang": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#!"
        },
        {
          "type": "PATTERN",
          "value": "[^\\n\\r]*"
        }
      ]
    },
    "single_line_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "//"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "nesting_comment"
    },
    {
      "type": "SYMBOL",
      "name": "single_line_comment"
    },
    {
      "type": "STRING",
      "value": "\r"
    },
    {
      "type": "STRING",
      "value": "\n"
    },
    {
      "type": "STRING",
      "value": "\t"
    },
    {
      "type": "STRING",
      "value": " "
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
